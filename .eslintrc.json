{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "no-param-reassign": ["error", { "props": false }],
        "no-alert": "off"
    },
    "globals": {
        "document": true,
        "window": true
    }
}